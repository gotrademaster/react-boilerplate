{
  "name": "frontend-template",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/core": "10.0.16",
    "@emotion/styled": "10.0.15",
    "@feathersjs/feathers": "4.3.1",
    "@feathersjs/rest-client": "4.3.1",
    "@rematch/core": "1.2.0",
    "axios": "0.19.0",
    "babel-eslint": "10.0.2",
    "compression": "1.7.4",
    "env-cmd": "10.0.1",
    "express": "4.17.1",
    "feathers-rematch": "0.1.2",
    "prop-types": "15.7.2",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-redux": "7.1.1",
    "react-router-dom": "5.0.1",
