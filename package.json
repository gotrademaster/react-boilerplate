    "start:development": "env-cmd -f ./environment/.env.development react-scripts start",
    "start:production": "env-cmd -f ./environment/.env.production react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
